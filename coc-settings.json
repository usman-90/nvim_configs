{
	"languageserver": {
		"sql": {
			"module": "/path/to/node_modules/sql-language-server/dist/bin/cli.js",
			"args": [
				"up",
				"--method",
				"node-ipc"
			],
			"filetypes": [
				"sql",
				"mysql"
			]
		},
		"clangd": {
			"command": "clangd",
			"rootPatterns": [
				"compile_flags.txt",
				"compile_commands.json"
			],
			"filetypes": [
				"c",
				"cc",
				"cpp",
				"c++",
				"objc",
				"objcpp"
			]
		},
	}
}
